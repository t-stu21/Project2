<div id="schedule-template" type="text/x-handlebars-template">

    <div class="row">
        <div class="container-fluid">
            <div class="app">
                <div class="row top-container">

                    <div class="circle-shape col-lg-2">
                        <div class="circle-text text-center">
                            <span class="title" id="kcal-title">
                                Todays Result
                            </span>
                            <span class="result">
                                <span class="num" id="kcal-today">
                                    <h1 id="circle-numbers">0000</h1>
                                </span>
                                <span class="unit" id="kcal">
                                    Kcal
                                </span>
                            </span>
                        </div>
                    </div>
                    <div class="col-lg-2"></div>
                    <div class="alyve col-md-4">
                        <div class="app-title text-center">

                            <div class="title-text"><span style="font-size: 90px; color: white;">
                                    <i class="fas fa-heartbeat"></i></span>ALYVE</div>
                        </div>
                    </div>
                    <div class="col-lg-2"></div>
                    <div class="circle-shape col-lg-2">
                        <div class="circle-text text-center">
                            <span class="title" id="kcal-title">
                                Goal:
                            </span>
                            <span class="result">
                                <span class="num" id="kcal-today">
                                    <h1 id="circle-numbers">0000</h1>
                                </span>
                                <span class="unit" id="kcal">
                                    Kcal
                                </span>
                            </span>
                        </div>
                    </div>



                </div>
                <div class="body">
                    <div class="attendance" id="goal">
                        {{#each schedule.days}}
                        <ul>
                            <li>{{name}}
                                <span><a href="/exercise" class="lyve">{{status}}</a></span>
                                <span><a href="/exercise" class="date">{{number}}</a></span>
                                <span class="circle present"></span>
                            </li>
                        </ul>
                        {{/each}}
                    </div>
                </div>



                <div>
                    <div class="card">
                        <h1>Daily Goals Met:</h1>
                        <p class="goalsmet">10</p>
                    </div>
                    <div class="card">
                        <h1>Alyve Score:</h1>
                        <p class="goalsmet">25896</p>
                    </div>
                    <div class="fat-burning">
                        <span class="title">
                            Todays Result
                        </span>
                        <span class="result">
                            <span class="num">0000</span>
                            <span class="unit">
                                Kcal
                            </span>
                        </span>
                    </div>

                </div>